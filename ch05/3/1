select * from sample51;
select name from sample51 group by name;
select name, count(name), sum(quantity) from sample51 group by name;

select name, count(name) from sample51 group by name;
select name, count(name) from sample51 group by name having count(name)=1; #having구로 검색조건 지정

select no, quantity from sample51 group by no, quantity; #GROUP BY에서 지정한 열 이외의 열은 집계함수를 사용하지 않을 경우 SELECT 구에 지정할 수 없음
select name, count(name), sum(quantity) from sample51 group by name order by sum(quantity) desc; #group by를 사용한다고 정렬되지 않음
